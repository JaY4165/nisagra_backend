package com.example.demo.repository;
import java.lang.annotation.Repeatable;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.example.demo.entity.EntityClass;


@Repository
public interface Repo extends CrudRepository<Integer,EntityClass > {
	@Query(value = "select * from nisagra.cart,nisagra.users where users.id=cart.user_id",nativeQuery = true)
	EntityClass findByEmail(String emailId);
	
	
	
	
 

}